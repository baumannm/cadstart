	<configoption>
		<id>graphic</id>
		<displayname_de>Grafikeinstellungen</displayname_de>
		<displayname_en>graphic settings</displayname_en>
		<subfolder>graphic</subfolder>
		<!-- <common>FALSE</common> -->
		<optionfile>
			<id>high</id>
			<displayname_de>Qualität</displayname_de>
			<displayname_en>quality</displayname_en>
			<filename>high.pro</filename>	
		</optionfile>
		<optionfile>
			<id>medium</id>
			<displayname_de>Mittel</displayname_de>
			<displayname_en>medium</displayname_en>
